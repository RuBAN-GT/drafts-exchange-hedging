{
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:node/recommended", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": ["node", "prettier"],
  "rules": {
    "no-console": 0,
    "node/no-extraneous-require": 0,
    "node/no-unpublished-require": 0,
    "node/no-unsupported-features": 0,
    "prettier/prettier": 1,
    "strict": 0
  }
}
